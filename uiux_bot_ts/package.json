{
  "name": "uiux_bot_ts",
  "version": "1.0.0",
  "description": "UI/UX Lessons Telegram Bot in TypeScript",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "start:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon",
    "dev:manual": "ts-node -r tsconfig-paths/register src/dev.ts",
    "build": "rimraf dist && tsc && tsc-alias",
    "build:prod": "npm run lint && npm run check-types && npm run test && npm run build",
    "migrate-to-supabase": "ts-node -r tsconfig-paths/register src/scripts/migrate-to-supabase.ts",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "check-types": "tsc --noEmit",
    "prepare": "npm run build",
    "health-check": "ts-node -r tsconfig-paths/register src/scripts/health-check.ts",
    "db:migrate": "ts-node src/database/run-migration.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "uiux",
    "lessons",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@supabase/supabase-js": "^2.39.4",
    "axios": "^1.6.7",
    "config": "^3.3.11",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.2",
    "googleapis": "^146.0.0",
    "grammy": "^1.20.3",
    "jsonrepair": "^3.12.0",
    "module-alias": "^2.2.3",
    "node-schedule": "^2.1.1",
    "openai": "^4.27.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^18.19.14",
    "@types/node-schedule": "^2.1.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
